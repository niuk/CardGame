{
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/ws": "^7.4.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "dependencies": {
    "await-semaphore": "0.1.3",
